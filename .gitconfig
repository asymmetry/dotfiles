[user]
    name = Chao Gu
    email = guchao.pku@gmail.com
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    sb = show-branch
[color]
    ui = true
    branch = auto
    diff = auto
    status = auto
[core]
    excludesfile = ~/.gitignore
    editor = nano
[push]
    default = upstream
[pull]
    ff = only
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
